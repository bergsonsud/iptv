<p id="notice"><%= notice %></p>

<h1>Mymails</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Expired</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mymails.each do |mymail| %>
      <tr>
        <td><%= mymail.email %></td>
        <td><%= mymail.expired %></td>
        <td><%= link_to 'Show', mymail %></td>
        <td><%= link_to 'Edit', edit_mymail_path(mymail) %></td>
        <td><%= link_to 'Destroy', mymail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mymail', new_mymail_path %>
